{
  "manifest_version": 3,
  "name": "PlayerHeadShot",
  "version": "1.0",
  "description": "A simple Chrome extension to search up players",
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "./src/popup.html"
  },
  "icons": {
    "16": "./assets/target16.png",
    "48": "./assets/target48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
